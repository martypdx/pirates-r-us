{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"modules": true,
			"experimentalObjectRestSpread": true,
			"impliedStrict": true
		}
	},
	"env": {
		"es6": true,
		"node": true,
		"mocha": true	
	},
	"globals": {
	
	},
	"rules": {
		"no-console": 0,
		"indent": [
			"error",
			4
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		]
	}
}